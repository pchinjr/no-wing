# Required environment variables for no-wing

# State directory for no-wing data and logs
NO_WING_STATE_DIR=./state

# AWS credentials file path
AWS_CREDENTIALS_PATH=~/.aws/credentials

# AWS endpoints (comma-separated)
AWS_ENDPOINTS=sts.amazonaws.com,iam.amazonaws.com

# AWS configuration
AWS_PROFILE=default
AWS_REGION=us-east-1

# Git configuration (optional)
GIT_AUTHOR_NAME=
GIT_AUTHOR_EMAIL=
