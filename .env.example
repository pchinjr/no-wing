# no-wing Environment Configuration
# Copy this to .env and fill in your values

# Project Configuration
PROJECT_NAME=your-name
GITHUB_REPO=owner/repo-name
AWS_REGION=us-east-1
ENVIRONMENT=dev

# AWS Configuration
AWS_ACCOUNT_ID=123456789012
AWS_PROFILE=default

# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_token_here

# Q Configuration
Q_ROLE_ARN=arn:aws:iam::ACCOUNT:role/QRole-your-name
Q_CAPABILITIES_LEVEL=1
Q_EXTERNAL_ID=q-your-name-dev

# Lambda Configuration (for control plane)
LAMBDA_FUNCTION_NAME=no-wing-control-plane
LAMBDA_REGION=us-east-1

# Optional: Advanced Configuration
DEBUG=false
LOG_LEVEL=info
DRY_RUN=false
