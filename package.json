{
  "name": "no-wing",
  "version": "1.0.0",
  "description": "Q Service Account Manager - Give Amazon Q its own identity for secure, auditable project automation",
  "main": "deno/no-wing.ts",
  "type": "module",
  "bin": {
    "no-wing": "./deno/no-wing.ts"
  },
  "scripts": {
    "dev": "deno run --allow-all deno/no-wing.ts",
    "start": "deno run --allow-all deno/no-wing.ts",
    "install-global": "./install-deno-final.sh",
    "test": "deno test --allow-all tests/no-wing.test.ts",
    "test:manual": "./test-manual.sh",
    "test:all": "deno test --allow-all tests/no-wing.test.ts && ./test-manual.sh",
    "test:credentials": "ts-node src/test/credential-test.ts",
    "test:permissions": "ts-node src/test/permission-test.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist .no-wing node_modules"
  },
  "keywords": [
    "amazon-q",
    "ai",
    "service-account",
    "security",
    "aws",
    "git",
    "identity",
    "isolation",
    "deno",
    "typescript",
    "automation",
    "audit"
  ],
  "author": "Paul Chin Jr <paul@paulchinjr.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pchinjr/no-wing.git"
  },
  "bugs": {
    "url": "https://github.com/pchinjr/no-wing/issues"
  },
  "homepage": "https://github.com/pchinjr/no-wing#readme",
  "engines": {
    "deno": ">=1.37.0"
  },
  "peerDependencies": {
    "deno": ">=1.37.0"
  },
  "files": [
    "deno/",
    "src/",
    "dist/",
    "install-deno-final.sh",
    "test-manual.sh",
    "tests/",
    "README.md",
    "LICENSE",
    "docs/",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.0.0",
    "@aws-sdk/client-cloudtrail": "^3.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
    "@aws-sdk/client-iam": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-sts": "^3.0.0",
    "@aws-sdk/credential-providers": "^3.0.0"
  }
}
